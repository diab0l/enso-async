polyglot java import experiments.Reflect
polyglot java import experiments.Aff
polyglot java import experiments.AffMonad
polyglot java import experiments.FileAsync

from Standard.Base import all

main =
	aContent = FileAsync.readFile "loremipsum.txt"
	comp = Aff.MonadInstance.bind aContent 
		content -> 
			Aff.MonadInstance.return_ 
				IO.println content
	Aff.runAff comp
